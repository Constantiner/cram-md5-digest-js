{
	"name": "@constantiner/cram-md5-digest",
	"version": "0.9.5",
	"description": "CRAM-MD5 digester implementation in JavaScript",
	"keywords": [
		"md5",
		"cryptography",
		"JavaScript",
		"ESNext",
		"ES6",
		"EcmaScript 2015",
		"CRAM-MD5"
	],
	"dependencies": {},
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.1.0",
		"@types/gulp": "^4.0.5",
		"@types/jest": "^23.3.3",
		"babel-preset-import-export": "^1.0.2",
		"del": "^3.0.0",
		"eslint": "^5.6.1",
		"eslint-config-prettier": "^3.1.0",
		"gulp": "^4.0.0",
		"gulp-better-rollup": "^3.3.0",
		"gulp-rename": "^1.4.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-uglify": "^3.0.1",
		"jest": "^23.6.0",
		"prettier": "^1.14.3",
		"rollup": "^0.66.5",
		"rollup-plugin-babel": "^4.0.3"
	},
	"scripts": {
		"test": "npx jest",
		"lint": "npx eslint '**/*.js'",
		"build": "npx gulp clean && npx gulp es6modules && npx gulp es5modules && npx gulp es5modulesMin",
		"format": "npx prettier 'src/*.js' --write",
		"prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
		"clean": "npx gulp clean",
		"clear_jest": "npx jest --clearCache"
	},
	"homepage": "https://github.com/Constantiner/cram-md5-digest-js.git",
	"author": "Konstantin Kovalev <constantiner@gmail.com>",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/Constantiner/cram-md5-digest-js.git",
		"type": "git"
	},
	"main": "cram-md5-digest",
	"jest": {
		"testPathIgnorePatterns": [
			"/node_modules/"
		],
		"coverageDirectory": "./coverage/",
		"collectCoverage": true,
		"collectCoverageFrom": [
			"src/**"
		]
	},
	"engines": {
		"node": ">=7.6",
		"npm": ">=5.2.0"
	},
	"prettier": {
		"printWidth": 120,
		"useTabs": true,
		"tabWidth": 4
	}
}
